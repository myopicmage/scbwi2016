<!DOCTYPE html>
<html ng-app="Scbwi" ng-cloak>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>SCBWI Florida</title>

    <link rel="stylesheet" href="~/lib/angular-material/angular-material.css">
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=RobotoDraft:300,400,500,700,400italic">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body layout="row" ng-controller="RegCtrl as main">
    <md-sidenav layout="column" class="md-sidenav-left md-whiteframe-z2" md-component-id="left" md-is-locked-open="$mdMedia('gt-md')">
        <md-toolbar class="md-toolbar md-hue-2">
            <h1 class="md-toolbar-tools">SCBWI Florida June</h1>
        </md-toolbar>
        <md-list class="left-menu">
            <md-subheader>Registration</md-subheader>
            <md-list-item>
                <a>
                    <md-item-content md-ink-ripple layout="row" layout-align="start center">
                        <div class="inset">
                            <md-button class="md-fab md-primary md-mini">
                                1
                            </md-button>
                        </div>
                        <div class="inset menu-text">
                            Basic Info
                        </div>
                    </md-item-content>
                </a>
            </md-list-item>
            <md-list-item>
                <a>
                    <md-item-content md-ink-ripple layout="row" layout-align="start center">
                        <div class="inset">
                            <md-button class="md-fab md-primary md-mini">
                                2
                            </md-button>
                        </div>
                        <div class="inset menu-text">
                            Package Selection
                        </div>
                    </md-item-content>
                </a>
            </md-list-item>
            <md-list-item>
                <a>
                    <md-item-content md-ink-ripple layout="row" layout-align="start center">
                        <div class="inset">
                            <md-button class="md-fab md-primary md-mini">
                                3
                            </md-button>
                        </div>
                        <div class="inset menu-text">
                            Choose Your Track
                        </div>
                    </md-item-content>
                </a>
            </md-list-item>
            <md-list-item>
                <a>
                    <md-item-content md-ink-ripple layout="row" layout-align="start center">
                        <div class="inset">
                            <md-button class="md-fab md-primary md-mini">
                                4
                            </md-button>
                        </div>
                        <div class="inset menu-text">
                            Verify
                        </div>
                    </md-item-content>
                </a>
            </md-list-item>
            <md-list-item>
                <a>
                    <md-item-content md-ink-ripple layout="row" layout-align="start center">
                        <div class="inset">
                            <md-button class="md-fab md-primary md-mini">
                                5
                            </md-button>
                        </div>
                        <div class="inset menu-text">
                            Submit & Pay
                        </div>
                    </md-item-content>
                </a>
            </md-list-item>
        </md-list>
    </md-sidenav>

    @RenderBody()


    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.2/angular.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.2/angular-animate.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.2/angular-messages.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.2/angular-aria.js"></script>
    <script src="~/lib/angular-material/angular-material.js"></script>

    <script src="~/js/app.js"></script>

    @RenderSection("scripts", required: false)

</body>
</html>
